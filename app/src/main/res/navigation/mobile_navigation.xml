<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.wil_byte_horizon.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_train"
        android:name="com.example.wil_byte_horizon.ui.train.TrainFragment"
        android:label="@string/title_training"
        tools:layout="@layout/fragment_train" />

    <fragment
        android:id="@+id/navigation_qualification"
        android:name="com.example.wil_byte_horizon.ui.qualif.QualificationFragment"
        android:label="@string/title_qualifications"
        tools:layout="@layout/fragment_qualifications" />

    <fragment
        android:id="@+id/navigation_soup"
        android:name="com.example.wil_byte_horizon.ui.admin.AdminDashboardFragment"
        android:label="@string/title_soup"
        tools:layout="@layout/fragment_admin_dashboard" />

    <fragment
        android:id="@+id/navigation_contact"
        android:name="com.example.wil_byte_horizon.ui.contact.ContactFragment"
        android:label="@string/title_contactus"
        tools:layout="@layout/fragment_contact" />
</navigation>